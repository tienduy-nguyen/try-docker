{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "CommonJS",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "baseUrl": "./",
    "outDir": "./dist",


  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}