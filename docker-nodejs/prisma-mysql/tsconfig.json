{
  "compilerOptions": {
    "target": "esNext",
    "typeRoots": [
      "./node_modules/@types",
      "src/@types"
    ],
    "lib": ["ESNext"],
    "module": "CommonJS",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "baseUrl": "./",
    "outDir": "./dist",
  },
  "include": ["./src", "prisma"],
  "exclude": ["node_modules"]
}